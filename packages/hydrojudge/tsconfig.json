{
    "compilerOptions": {
        "target": "es2018",
        "module": "commonjs",
        "outDir": ".",
        "rootDir": ".",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "allowJs": false,
        "resolveJsonModule": true
    },
    "include": [
        "service.ts",
        "src"
    ],
    "paths": {
        "src/*": [
            "dist/*"
        ]
    },
    "exclude": []
}